@inproceedings{chen:2007,
	Abstract = {In this paper, the available data logs collected from taxis are used to analyze the temporal distribution characteristics of the travel time reliability on the road network in Beijing. Three different measures are applied to quantify the travel time reliability during different time-of-the-day (TOD) and day-of-the-week (DOW) periods, namely the Coefficient of Variations (CV), the Planning Rate Index (PRI), and the Probability Indicator (PI). The probability indicator is a novel measure developed in this paper to evaluate the reliability of a road network, which is defined as the probability that the travel rate (travel time per unit distance) is less than a threshold value. The paper then discusses the effectiveness of these measures from the analytical standpoint. The travel rate distribution at an AM peak hour on Wednesday is modeled and the result indicates that a lognormal distribution shows better approximation performance. It is observed that both skewness and width of the travel rate distribution are related to the travel time reliability. Therefore, the probability indicator, considering both width and skewness of the travel rate distribution, is more robust for assessing the travel time reliability than the coefficient of variation and the planning rate index which merely consider the width of the travel rate distribution. Finally, the characteristics analysis of the travel time reliability for different TOD-DOW periods demonstrates that there is a strong relationship between the reliability and congestion. The reliability is low during the peak hours when the congested conditions occur, whereas the reliability is high during the off-peak hours.},
	Author = {Chen, Kun and Yu, Lei and Guo, Jifu and Wen, Huimin},
	Booktitle = {Transportation Research Board 86th Annual Meeting},
	Date-Added = {2013-06-12 03:29:04 +0000},
	Date-Modified = {2013-06-12 03:29:04 +0000},
	Number = {07-1420},
	Title = {Characteristics analysis of road network reliability in {B}eijing based on the data logs from taxis},
	Year = {2007}}

@inproceedings{rakha:2006,
	Abstract = {The estimation of path or trip travel-time reliability is critical to any advanced traveler information system. The state-of-practice procedures for estimating path travel-time reliability assumes that travel times follow a normal distribution and requires a measure of trip travel-time variance. The study analyzes AVI data from San Antonio and demonstrates through goodness-of-fit tests that the assumption of normality is, from a theoretical standpoint, inconsistent with field travel-time observations and that a lognormal distribution is more representative of roadway travel times. However, visual inspection of the data demonstrates that the normality assumption may be sufficient from a practical standpoint given its computational simplicity. The paper then proposes five methods for the estimation of path travel-time variance from its component segment travel-time variances. The analysis demonstrates that computing the trip travel-time coefficient of variation as the conditional expectation over all realizations of roadway segments provides estimates within 13% of field observations for both uncongested and congested conditions.},
	Author = {Rakha, Hesham and El-Shawarby, Ihab and Arafeh, Mazen and Dion, Francois},
	Booktitle = {Intelligent Transportation Systems Conference, 2006. ITSC'06. IEEE},
	Date-Added = {2013-06-12 03:28:54 +0000},
	Date-Modified = {2013-06-12 03:28:54 +0000},
	Organization = {IEEE},
	Pages = {236--241},
	Title = {Estimating path travel-time reliability},
	Year = {2006}}

